#########################
##### Project

PROJECT_NAME           = @PROJECT_FULLNAME@
PROJECT_NUMBER         = @PROJECT_VERSION@
PROJECT_BRIEF          = @PROJECT_DESCRIPTION@
PROJECT_LOGO           = @CMAKE_SOURCE_DIR@/doc/logo.png
OUTPUT_DIRECTORY       = @CMAKE_BINARY_DIR@/doc/manual
CREATE_SUBDIRS         = NO
INLINE_INHERITED_MEMB  = YES
STRIP_FROM_PATH        = @CMAKE_SOURCE_DIR@/src
JAVADOC_AUTOBRIEF      = YES

ALIASES                = "rcomplexity=\xrefitem rcomplexity \"Runtime complexity\" \"Runtime Complexity Bounds\""
ALIASES                += "complexity=\xrefitem rcomplexity \"Runtime complexity\" \"Runtime Complexity Bounds\""
ALIASES                += "scomplexity=\xrefitem scomplexity \"Space complexity\" \"Space Complexity Bounds\""
ALIASES                += startcomment="\htmlonly/&#42;&#42;\endhtmlonly\latexonly$/\ast\ast$\endlatexonly"
ALIASES                += endcomment="\htmlonly&#42;/\endhtmlonly\latexonly$\ast$/\endlatexonly"

OPTIMIZE_OUTPUT_FOR_C  = YES
MARKDOWN_SUPPORT       = YES
BUILTIN_STL_SUPPORT    = YES
DISTRIBUTE_GROUP_DOC   = YES

#########################
##### Build

EXTRACT_ALL            = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = NO
SHOW_USED_FILES        = NO
SHOW_FILES             = NO
SHOW_NAMESPACES        = NO
CITE_BIB_FILES         = @CMAKE_SOURCE_DIR@/doc/literature.bib @CMAKE_SOURCE_DIR@/doc/literature-theses.bib

#########################
##### Warnings and progress

#########################
##### Input

INPUT                  = @PROJECT_SOURCE_DIR@/doc/manual @PROJECT_SOURCE_DIR@/src/smtrat-modules @PROJECT_SOURCE_DIR@/src/smtrat-solver
FILE_PATTERNS          = *.cpp *.h *.tpp *.dox *.py *.md
RECURSIVE              = YES
EXCLUDE_PATTERNS       = */tests/* */smtrat-modules/*Module/*Settings.h */smtrat-strategies/strategies/*
EXCLUDE_SYMBOLS        = boost*
EXAMPLE_PATH           = examples/
EXAMPLE_RECURSIVE      = YES

#########################
##### Source browsing

CLANG_ASSISTED_PARSING = YES
CLANG_DATABASE_PATH    = @CMAKE_BINARY_DIR@/compile_commands.json

#########################
##### Alphabetical index

ALPHABETICAL_INDEX     = NO
COLS_IN_ALPHA_INDEX    = 5
IGNORE_PREFIX          =

#########################
##### LaTeX output

GENERATE_LATEX         = YES
LATEX_OUTPUT           = @CMAKE_BINARY_DIR@/doc/manual
COMPACT_LATEX          = YES
LATEX_HEADER           = @PROJECT_SOURCE_DIR@/doc/manual/header.tex
LATEX_FOOTER           = @PROJECT_SOURCE_DIR@/doc/manual/footer.tex
LATEX_EXTRA_FILES      = @PROJECT_SOURCE_DIR@/doc/manual/doxygen.sty
PDF_HYPERLINKS         = YES
LATEX_BATCHMODE        = NO
LATEX_HIDE_INDICES     = NO
LATEX_BIB_STYLE        = plainnat

#########################
##### Other outputs

GENERATE_HTML          = NO
GENERATE_RTF           = NO
GENERATE_MAN           = NO
GENERATE_XML           = NO
GENERATE_DOCBOOK       = NO
GENERATE_AUTOGEN_DEF   = NO
GENERATE_PERLMOD       = NO
GENERATE_TAGFILE       = 

#########################
##### Preprocessor

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES
#INCLUDE_PATH           = .
PREDEFINED             = _DOXYGEN DEBUG
EXPAND_AS_DEFINED      = EASY_TYPE \
                         EASY_DESTRUCTOR \
                         EASY_METHOD \
                         TRAIT_TRUE \
                         TRAIT_FALSE \
                         TRAIT_TYPE

#########################
##### Dot

HAVE_DOT               = YES
CLASS_GRAPH            = NO
COLLABORATION_GRAPH    = NO
GROUP_GRAPHS           = NO
UML_LOOK               = NO
TEMPLATE_RELATIONS     = NO
INCLUDE_GRAPH          = NO
INCLUDED_BY_GRAPH      = NO
CALL_GRAPH             = NO
CALLER_GRAPH           = NO
GRAPHICAL_HIERARCHY    = NO
DIRECTORY_GRAPH        = NO
DOT_PATH               = @DOT_PATH@
DOT_GRAPH_MAX_NODES    = 50
DOT_TRANSPARENT        = YES
DOT_MULTI_TARGETS      = YES
GENERATE_LEGEND        = YES
